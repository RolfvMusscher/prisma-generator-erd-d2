{
  "name": "prisma-generator-erd-d2",
  "description": "Provide a description about your generator",
  "version": "1.1.6",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-erd-d2": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "@prisma/generator-helper": "6.16.2",
    "@prisma/sdk": "4.0.0",
    "prettier": "3.6.2"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "@types/prettier": "3.0.0",
    "jest": "30.1.3",
    "prisma": "6.16.2",
    "ts-jest": "29.4.2",
    "typescript": "5.9.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^24.0.0"
  },
  "homepage": "https://github.com/RolfvMusscher/prisma-generator-erd-d2",
  "repository": {
    "type": "git",
    "url": "https://github.com/RolfvMusscher/prisma-generator-erd-d2"
  },
  "author": "Rolf van Musscher <rolf.van.musscher@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "d2"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
